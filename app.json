{
  "expo": {
    "name": "SYNC",
    "slug": "sync-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.sync.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0F"
      },
      "package": "com.sync.app"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video"
    ],
    "scheme": "sync",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eac426b3-55ca-4fcf-a18e-44756dafae07"
      }
    },
    "owner": "meherzan"
  }
}
